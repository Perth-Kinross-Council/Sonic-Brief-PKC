{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "enabled": true,
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/__TENANT_ID__/v2.0",
          "clientSecretSettingName": "IGNORED"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/login",
      "allowedRoles": ["anonymous"],
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/*",
      "/images/*",
      "/*.css",
      "/*.js"
    ]
  }
}
